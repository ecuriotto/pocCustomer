    "exp_prepareCases" : {
      "type" : "expression",
      "value" : "var output = [];\nif($data.cases && $data.archivedCases){\n    $data.cases.forEach(function(item){\n       var c = angular.copy(item);\n       c.caseId = c.id;\n       c.state = \"On Execution\";\n       output.push(c)\n    });\n    $data.archivedCases.forEach(function(item){\n       var c = angular.copy(item);\n       c.caseId = c.sourceObjectId;\n       c.state = \"Completed\";\n       output.push(c)\n    });\n    return output;\n}",
